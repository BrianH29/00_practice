package com.hw1.run;

import com.hw1.model.vo.Employee;

public class Run {

	public static void main(String[] args) {
		
		Employee[] emp = new Employee[3];
		
		emp[0] = new Employee();
		emp[1] = new Employee(123,"홍길동",29,'M',"416-450-6581","광진구 구의동");
		emp[2] = new Employee(216,"김말순","기획부","강사",45,'F',2000000,0.02,"011-895-6598","서초구");
		
		System.out.println(emp[0].info());
		System.out.println(emp[1].info());
		System.out.println(emp[2].info());
	}
}

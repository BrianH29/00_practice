package com.hw2.model.vo;

public class Point {

	private int x;
	private int y; 
	
	public Point() {
		
	}
	
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public void draw(Point[] p1) {
		for(int i=0;i<p1.length;i++) {
			System.out.println(p1[i] + " ");
		}
		System.out.println();
	}
	
	//setter
	public void setX() {
		this.x = x; 
	}
	public void setY() {
		this.y= y;
	}
	
	//getter
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	
	public int info() {
		return x + y ; 
	}
	
}
